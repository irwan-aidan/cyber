port 1194
proto tcp4
dev tun
tun-mtu 1500
mssfix 1500

ca keys/ca.crt
cert keys/VoltVpn.crt
key keys/VoltVpn.key
dh keys/dh2048.pem

plugin /usr/lib/openvpn/openvpn-plugin-auth-pam.so login
username-as-common-name
client-cert-not-required
cipher none
auth none

server 10.6.0.0 255.255.255.0
push "redirect-gateway def1"
push "dhcp-option DNS 8.8.8.8"
push "dhcp-option DNS 8.8.4.4"
push "sndbuf 393216"
push "rcvbuf 393216"
sndbuf 0
rcvbuf 0
topology subnet

management 127.0.0.1 446
status openvpn-status.log
#log openvpn.log
keepalive 60 180
ping-timer-rem
reneg-sec 0
tcp-nodelay
persist-key
persist-tun
comp-lzo
comp-noadapt
verb 3
